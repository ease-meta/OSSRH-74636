<project xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>com.open.cloud</groupId>
	<artifactId>open-cloud-platform</artifactId>
	<version>0.0.1-SNAPSHOT</version>
	<packaging>pom</packaging>


	<repositories>
		<repository>
			<id>aliyun</id>
			<url>http://maven.aliyun.com/nexus/content/groups/public</url>
		</repository>
		<repository>
			<id>repo2</id>
			<url>http://repo2.maven.org/maven2/</url>
		</repository>
		<repository>
			<id>spring-milestones</id>
			<name>Spring Milestones</name>
			<url>https://repo.spring.io/milestone</url>
		</repository>
		<repository>
			<id>cloudera</id>
			<url>https://repository.cloudera.com/artifactory/cloudera-repos/</url>
		</repository>
		<repository>
			<id>scala-tools.org</id>
			<name>Scala-Tools Maven2 Repository</name>
			<url>http://scala-tools.org/repo-releases</url>
		</repository>
		<repository>
			<id>apache.snapshots</id>
			<name>Apache Development Snapshot Repository</name>
			<url>https://repository.apache.org/content/repositories/snapshots/</url>
			<releases>
				<enabled>false</enabled>
			</releases>
			<snapshots>
				<enabled>true</enabled>
			</snapshots>
		</repository>

	</repositories>

	<properties>
		<spring-cloud.version>Greenwich.RC2</spring-cloud.version>
		<spring-cloud-zookeeper.version>2.1.0.RC2</spring-cloud-zookeeper.version>
		<project.encoding>UTF-8</project.encoding>
		<jdk.version>1.8</jdk.version>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
		<java.version>1.8</java.version>
		<!--验证码 -->
		<kaptcha.version>2.3.2</kaptcha.version>
		<shiro.version>1.4.0</shiro.version>
		<thymeleaf.extras.shiro.version>2.0.0</thymeleaf.extras.shiro.version>
		<!-- 解析客户端操作系统、浏览器等 -->
		<bitwalker.version>1.19</bitwalker.version>
		<!-- pagehelper 分页插件 -->
		<pagehelper.boot.version>1.2.5</pagehelper.boot.version>
		<!-- 获取系统信息 -->
		<oshi.version>3.9.1</oshi.version>
		<swagger.version>2.7.0</swagger.version>

		<hutool-all.version>4.3.2</hutool-all.version>
		<QLExpress.version>3.2.0</QLExpress.version>

		<arthas-all.version>3.0.4</arthas-all.version>
		<druid.version>1.1.12</druid.version>
		<dubbo.version>2.6.5</dubbo.version>
		<fastjson.version>1.2.54</fastjson.version>
		<druid-spring-boot-starter.version>1.1.10</druid-spring-boot-starter.version>
		<dubbo-spring-boot-starter.version>0.1.0</dubbo-spring-boot-starter.version>
		<simpleimage.version>1.2.3</simpleimage.version>
		<tac-engine.version>0.0.4</tac-engine.version>
		<easyexcel.version>1.1.1</easyexcel.version>
		<transmittable.version>2.8.1</transmittable.version>
		<mybatis-spring.version>1.3.2</mybatis-spring.version>
		<mybatis.version>3.4.6</mybatis.version>

		<cglib.version>3.2.8</cglib.version>
		<jodd.version>5.0.5</jodd.version>
		<pentaho-kettle.version>7.1.0.0-12</pentaho-kettle.version>

		<lombok.version>1.18.2</lombok.version>

		<commons-lang.version>2.6</commons-lang.version>
		<commons-lang3.version>3.8.1</commons-lang3.version>
		<commons-configuration.version>1.10</commons-configuration.version>
		<commons-configuration2.version>2.3</commons-configuration2.version>
		<commons-collections.version>3.2.2</commons-collections.version>
		<commons-collections4.version>4.2</commons-collections4.version>
		<commons-io.version>2.6</commons-io.version>
		<commons-net.version>3.6</commons-net.version>
		<jsch.version>0.1.54</jsch.version>
		<shiro.version>1.4.0</shiro.version>

		<latest.release.version>2.1.5</latest.release.version>

		<guava.version>26.0-jre</guava.version>
		<slf4j-version>1.7.25</slf4j-version>
		<dom4j.version>1.6.1</dom4j.version>
		<okhttp.version>3.11.0</okhttp.version>
		<clj-http.version>3.9.1</clj-http.version>
		<itextpdf.version>5.5.13</itextpdf.version>
		<itext-asian.version>5.2.0</itext-asian.version>
		<hibernate-validator.version>6.0.13.Final</hibernate-validator.version>
		<easypoi.version>3.2.0</easypoi.version>
		<asypoispringboot.version>0.1.1</asypoispringboot.version>
		<ooxml-schemas.version>1.3</ooxml-schemas.version>
		<poi.version>3.17</poi.version>
		<ooxml-security.version>1.1</ooxml-security.version>
		<xerces.version>2.9.1</xerces.version>
		<bval.bundle.version>1.1.0</bval.bundle.version>
		<jmh.version>1.21</jmh.version>
		<jquery.version>3.3.1</jquery.version>
		<bootstrap.version>4.1.3</bootstrap.version>
		<jsr305.version>3.0.2</jsr305.version>
		<findbugs.version>3.0.1</findbugs.version>
		<dangdang-sharding-jdbc.version>1.5.4.1</dangdang-sharding-jdbc.version>
		<simple-jndi.version>0.11.4.1</simple-jndi.version>
		<zxing.version>3.3.3</zxing.version>
		<findbugs.version>3.0.2</findbugs.version>
		<mysql.version>5.1.47</mysql.version>
		<joda-time.version>2.10</joda-time.version>
	</properties>

	<dependencyManagement>
		<dependencies>
			<dependency>
				<groupId>org.springframework.boot</groupId>
				<artifactId>spring-boot-dependencies</artifactId>
				<version>2.1.0.RELEASE</version>
				<type>pom</type>
				<scope>import</scope>
			</dependency>
			<dependency>
				<groupId>org.springframework.cloud</groupId>
				<artifactId>spring-cloud-dependencies</artifactId>
				<version>${spring-cloud.version}</version>
				<type>pom</type>
				<scope>import</scope>
			</dependency>
			<dependency>
				<groupId>org.springframework.cloud</groupId>
				<artifactId>spring-cloud-alibaba-dependencies</artifactId>
				<version>0.2.1.RELEASE</version>
				<type>pom</type>
				<scope>import</scope>
			</dependency>
		</dependencies>
	</dependencyManagement>
	<dependencies>

		<dependency>
			<groupId>mysql</groupId>
			<artifactId>mysql-connector-java</artifactId>
		</dependency>
		<dependency>
			<groupId>org.projectlombok</groupId>
			<artifactId>lombok</artifactId>
			<scope>provided</scope>
		</dependency>

		<dependency>
			<groupId>com.leijian</groupId>
			<artifactId>moc</artifactId>
			<version>0.0.1</version>
		</dependency>
	</dependencies>
	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<configuration>
					<source>${java.version}</source>
					<target>${java.version}</target>
					<encoding>${project.build.sourceEncoding}</encoding>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.springframework.boot</groupId>
				<artifactId>spring-boot-maven-plugin</artifactId>
				<configuration>
					<fork>true</fork> <!-- 如果没有该配置，devtools不会生效 -->
				</configuration>
			</plugin>
		</plugins>
		<finalName>${artifactId}</finalName>
	</build>
	<modules>
		<module>cloud-config-center</module>
		<module>cloud-register-center</module>
	</modules>
</project>