<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:jsf="http://jsf.jd.com/schema/jsf"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans.xsd
http://jsf.jd.com/schema/jsf http://jsf.jd.com/schema/jsf/jsf.xsd">
    <!-- 注册中心 : index 服务地址 -->
    <!-- 注册中心： 配置 index 服务地址，使用 index 服务寻址注册中心地址 -->
    <!--<jsf:registry id="jsfRegistry" protocol="jsfRegistry" index="i.jsf.jd.com"/>-->
    <jsf:registry id="jsfRegistry" protocol="fileRegistry" file="/app/dcits/jsf/" index="i.jsf.jd.com"/>
    <!-- 服务调用者配置直连使用 url="jsf://IP:PORT" -->
    <jsf:consumer id="helloService" interface="com.open.cloud.jsf.api.IHelloService"
                  protocol="jsf" alias="CHANGE-IT" timeout="10000" injvm="false" cluster="failfast">
    </jsf:consumer>
</beans>