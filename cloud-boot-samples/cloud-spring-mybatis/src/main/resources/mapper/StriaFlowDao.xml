<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.open.cloud.spring.mybatis.dao.StriaFlowDao">

    <resultMap type="com.open.cloud.spring.mybatis.entity.StriaFlow" id="StriaFlowMap">
        <result property="flowid" column="FLOWID" jdbcType="VARCHAR"/>
        <result property="flowType" column="FLOW_TYPE" jdbcType="VARCHAR"/>
        <result property="title" column="TITLE" jdbcType="VARCHAR"/>
        <result property="initNum" column="INIT_NUM" jdbcType="NUMERIC"/>
        <result property="version" column="VERSION" jdbcType="VARCHAR"/>
        <result property="state" column="STATE" jdbcType="NUMERIC"/>
        <result property="createtime" column="CREATETIME" jdbcType="VARCHAR"/>
        <result property="creator" column="CREATOR" jdbcType="VARCHAR"/>
        <result property="dtpFlag" column="DTP_FLAG" jdbcType="VARCHAR"/>
        <result property="timeOut" column="TIME_OUT" jdbcType="NUMERIC"/>
        <result property="prodType" column="PROD_TYPE" jdbcType="VARCHAR"/>
        <result property="eventType" column="EVENT_TYPE" jdbcType="VARCHAR"/>
        <result property="txFlag" column="TX_FLAG" jdbcType="VARCHAR"/>
        <result property="separateRw" column="SEPARATE_RW" jdbcType="VARCHAR"/>
    </resultMap>

    <!--通过实体作为筛选条件查询-->
    <select id="queryAll" resultMap="StriaFlowMap">
        select
        FLOWID, FLOW_TYPE, TITLE, INIT_NUM, VERSION, STATE, CREATETIME, CREATOR, DTP_FLAG, TIME_OUT, PROD_TYPE,
        EVENT_TYPE, TX_FLAG, SEPARATE_RW
        from stria_flow
        <where>
            <if test="flowid != null and flowid != ''">
                and FLOWID = #{flowid}
            </if>
            <if test="flowType != null and flowType != ''">
                and FLOW_TYPE = #{flowType}
            </if>
            <if test="title != null and title != ''">
                and TITLE = #{title}
            </if>
            <if test="initNum != null">
                and INIT_NUM = #{initNum}
            </if>
            <if test="version != null and version != ''">
                and VERSION = #{version}
            </if>
            <if test="state != null">
                and STATE = #{state}
            </if>
            <if test="createtime != null and createtime != ''">
                and CREATETIME = #{createtime}
            </if>
            <if test="creator != null and creator != ''">
                and CREATOR = #{creator}
            </if>
            <if test="dtpFlag != null and dtpFlag != ''">
                and DTP_FLAG = #{dtpFlag}
            </if>
            <if test="timeOut != null">
                and TIME_OUT = #{timeOut}
            </if>
            <if test="prodType != null and prodType != ''">
                and PROD_TYPE = #{prodType}
            </if>
            <if test="eventType != null and eventType != ''">
                and EVENT_TYPE = #{eventType}
            </if>
            <if test="txFlag != null and txFlag != ''">
                and TX_FLAG = #{txFlag}
            </if>
            <if test="separateRw != null and separateRw != ''">
                and SEPARATE_RW = #{separateRw}
            </if>
        </where>
    </select>
</mapper>