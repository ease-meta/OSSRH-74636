<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.open.cloud.prod">

    <!-- 插入流水表CM_TRAN_SEQ -->
    <insert id="InsAccount" parameterType="hashMap">
   <![CDATA[



     insert into ACCOUNT (
        ACCOUNT_NO,AMOUNT,FREEZED_AMOUNT
        ) values (
        #{ACCOUNT_NO},#{AMOUNT},#{FREEZED_AMOUNT})


        ]]>
	</insert>

    <delete id="DelAccount" parameterType="hashMap">
    <![CDATA[

     delete from ACCOUNT where ACCOUNT_NO =
       #{ACCOUNT_NO}

        ]]>
    </delete>


    <insert id="InsAccountPoint" parameterType="hashMap">
   <![CDATA[



     insert into ACCOUNT_POINT (
        TX_ID,ACCOUNT_NO,POINT,STATUS
        ) values (
        #{TX_ID},#{ACCOUNT_NO},#{POINT},#{STATUS})


        ]]>
	</insert>

</mapper>