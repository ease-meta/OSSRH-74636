<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">
    <entity class="com.open.cloud.leaf.segment.model.LeafAlloc">
        <table name="leaf_alloc" schema="sloars" catalog=""/>
        <attributes>
            <id name="bizTag">
                <column name="biz_tag" length="128"/>
            </id>
            <basic name="maxId">
                <column name="max_id" precision="20"/>
            </basic>
            <basic name="step">
                <column name="step" precision="11"/>
            </basic>
            <basic name="description">
                <column name="description" nullable="false" length="256"/>
            </basic>
            <basic name="updateTime">
                <column name="update_time" precision="-1"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
